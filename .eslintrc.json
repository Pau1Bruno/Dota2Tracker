{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "plugins": ["react", "import"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", "internal"],
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
